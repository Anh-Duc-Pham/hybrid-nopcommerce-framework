<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Facebook Automation Testing">
    <listeners>
        <listener class-name="commons.MethodListener"/>
        <listener class-name="reportConfigs.AllureTestListener"/>
    </listeners>

    <parameter name="url" value="https://www.facebook.com/"/>
    <test name="Run on Firefox browser">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="com.facebook.home.Lever_20_Undisplayed" />
        </classes>
    </test>


</suite>