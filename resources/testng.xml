<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="NopCommerce Automation Testing">

    <test name="Run on Firefox browser">
        <parameter name="browser" value="firefox"/>
        <classes>
<!--            <class name="com.nopcommerce.users.Level_01_Dry" />-->
            <class name="com.nopcommerce.users.Lever_04_MultipleBrowser" />
        </classes>
    </test>

    <test name="Run on Chrome browser">
        <parameter name="browser" value="chrome"/>
        <classes>
            <!--            <class name="com.nopcommerce.users.Level_01_Dry" />-->
            <class name="com.nopcommerce.users.Lever_04_MultipleBrowser" />
        </classes>
    </test>

    <test name="Run on Edge browser">
        <parameter name="browser" value="edge"/>
        <classes>
            <!--            <class name="com.nopcommerce.users.Level_01_Dry" />-->
            <class name="com.nopcommerce.users.Lever_04_MultipleBrowser" />
        </classes>
    </test>
</suite>